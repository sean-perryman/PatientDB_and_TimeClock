<div class="container well">
  <div class="col-md-6">

    <h1>Listing Time Entries</h1>

    <table>
      <thead>
        <tr>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @timeentries.each do |timeentry| %>
          <tr>
            <td><%= timeentry.user_id %></td>
            <td><%= timeentry.time_in %></td>
            <td><%= timeentry.time_out %></td>
            <td><%= link_to 'Edit', edit_timeentry_path(timeentry) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

  </div>
</div>